{
	"name": "slash to search",
	"version": "0.2",
	"description": "Fasten your speed to search!!",
	"icons":{"48": "icon.png", "16": "icon.png"},
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["s2s.js", "jquery.js"]
		}	
	],
	"permissions": [
		"tabs", "*://*/*","contextMenus"
	],
	"options_page": "options.html",
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Slash-to-Search"
	},
	"background_page": "bg.html",
	"plugins": [
		{"path": "plugins/npSwitchy.dll"},
		{"path": "plugins/npSwitchy.so"},
		{"path": "plugins/npSwitchy64.so"},
		{"path": "plugins/iSwitchy.bundle"}
	]
}
